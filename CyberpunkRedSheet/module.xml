<?xml version="1.0" encoding="UTF-8"?>
<module sdkVersion="3.6c">
    <!--The "id" identifies your plugin globally. Must be unique, must contain only alphanumeric characters, underlines and dots and must contain at least 5 characters and a maximum of 40. Once set the ID, you should not change it ;)-->
    <id>UNIQUE_PLUGIN_IDENTIFIER</id>
    <!--"version" is the version of your plugin. When you release a new version of your module, change here =)-->
    <version>1.0</version>
    <info lang="en-US">
        <!--Plugin information in "en-US" language. You can have multiple "info" tags, one for each translation you want.-->
        <name>Short name of your plugin here. Example: DnD 5e</name>
        <description>Description of the plugin here. Example: Adds tab support for the ETC system, with such features, for such purposes ...</description>
        <author>Your name here</author>
        <site>http://www.yourwebsite.com</site>
        <contact>Facebook? Twitter? E-Mail?</contact>
    </info>
</module>
